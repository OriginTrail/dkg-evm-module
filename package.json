  "version": "3.1.2",
  "description": "",
  "main": "truffle-config.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "truffle test",
    "coverage": "truffle run coverage",
    "compile": "truffle compile",
    "prettier:solidity": "./node_modules/.bin/prettier --write contracts/**/*.sol",
    "ganache": "ganache -e 1000000000000 -i 5777 -p 7545 -l 10000000 -a 30 -m \"aspect ask story desert profit engage tuition leave fade giraffe exclude brief\"",
    "deploy": "truffle migrate --network ganache --reset",
    "deploy:rinkeby": "truffle migrate --network rinkeby --reset",
    "deploy:mumbai": "truffle migrate --network mumbai --reset",
    "truffle_deploy:otp_devnet": "truffle migrate --network otp_devnet --reset",
    "truffle_deploy:otp_testnet": "truffle migrate --network otp_testnet --reset",
    "truffle_deploy:otp_mainnet": "truffle migrate --network otp_mainnet --reset",
    "deploy:devnet": "node scripts/deploy.js --environment otp_devnet",
    "deploy:testnet": "node scripts/deploy.js --environment otp_testnet"
  },
  "author": "",
  "license": "ISC",
