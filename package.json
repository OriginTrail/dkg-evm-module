    "chai": "^4.3.6",
    "dotenv": "^16.0.2",
    "eslint": "^8.23.0",
    "ethers": "^5.7.2",
    "ganache": "^7.4.4",
    "solhint": "^3.3.7",
    "truffle": "^5.6.0"
  },
